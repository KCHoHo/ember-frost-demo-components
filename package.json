{
  "name": "ember-frost-demo-components",
  "version": "3.1.5",
  "description": "A collection of components and blueprints for showcasing frost components",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": "git@github.com:ciena-frost/ember-frost-demo-components.git",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Sophy Pal <sophypal>",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-cli": "2.12.3",
    "ember-cli-chai": "0.4.3",
    "ember-cli-code-coverage": "0.3.12",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-frost-blueprints": "^5.0.1",
    "ember-cli-htmlbars-inline-precompile": "0.3.12",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.14.4",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-frost-test": "^4.0.0",
    "ember-hook": "1.4.2",
    "ember-load-initializers": "^0.6.0",
    "ember-prism": "0.0.8",
    "ember-resolver": "^2.0.3",
    "ember-sinon": "^0.7.0",
    "ember-source": "~2.12.0",
    "ember-spread": "^1.2.2",
    "ember-test-utils": "^8.1.0",
    "ivy-codemirror": "^2.1.0",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.14.0"
  },
  "keywords": [
    "ember-addon",
    "frost"
  ],
  "dependencies": {
    "broccoli-babel-transpiler": "^5.7.2",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^2.0.0",
    "ember-browserify": "^1.2.0",
    "ember-cli-babel": "^5.1.7",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-sass": "7.1.1",
    "ember-cli-showdown": "^2.11.0",
    "ember-cli-version-checker": "~2.0.0",
    "ember-computed-decorators": "0.3.0",
    "ember-frost-core": "^5.1.1",
    "ember-frost-popover": "^8.0.0",
    "ember-prop-types": "^6.0.1",
    "ember-truth-helpers": "^1.3.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "ember-frost-icon-pack": {
    "name": "frost-demo-components"
  }
}
